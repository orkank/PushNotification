<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <idangerous_pushnotification>
            <firebase>
                <enabled>0</enabled>
                <project_id/>
                <service_account_json backend_model="Magento\Config\Model\Config\Backend\Encrypted"/>
                <server_key backend_model="Magento\Config\Model\Config\Backend\Encrypted"/>
            </firebase>
            <general>
                <max_tokens_per_request>1000</max_tokens_per_request>
                <notification_sound>default</notification_sound>
            </general>
        </idangerous_pushnotification>
    </default>
</config>

